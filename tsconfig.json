{
  "extends": "./tsconfig.paths.json",
  "compilerOptions": {
    "target": "ES6",
    "lib": ["DOM", "ES6", "ESNext"],
    "jsx": "react-jsx",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "typeRoots": ["./node_modules/@types", "./src/types"]
  }
}
